generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions {
  id         String   @id
  title      String
  type       String
  value      Float
  category   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("transactions")
}
