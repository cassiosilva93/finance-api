generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions {
  id         String   @id
  title      String
  type       String
  value      Float
  category   String
  created_at DateTime
  updated_at DateTime

  @@map("transactions")
}

model Users {
  id         String   @id
  name       String
  email      String
  password   String
  created_at DateTime
  updated_at DateTime

  @@map("users")
}
